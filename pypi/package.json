{
  "name": "xregistry-pypi-wrapper",
  "version": "1.0.0",
  "description": "xRegistry API wrapper for PyPI",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc && npm run copy:shared",
    "copy:shared": "node -e \"require('fs').cpSync('../shared/filter', 'dist/shared/filter', { recursive: true })\"",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --ext ts --exec \"npm run build && npm start\"",
    "type-check": "tsc --noEmit"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "express": "^4.18.2",
    "yargs": "^18.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.10.1",
    "@types/uuid": "^9.0.7",
    "@types/yargs": "^17.0.35",
    "nodemon": "^3.1.11",
    "rimraf": "^5.0.5",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}