{
    "name": "xregistry-npm-wrapper",
    "version": "1.0.0",
    "description": "xRegistry wrapper for NPM packages",
    "main": "dist/server.js",
    "types": "dist/server.d.ts",
    "scripts": {
        "build": "tsc && npm run copy:shared",
        "copy:shared": "node -e \"require('fs').cpSync('../shared/filter', 'dist/shared/filter', { recursive: true })\"",
        "build:watch": "tsc --watch",
        "start": "node dist/server.js",
        "start:dev": "ts-node src/server.ts",
        "start:watch": "ts-node --watch src/server.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "clean": "rimraf dist",
        "prebuild": "npm run clean",
        "prestart": "npm run build"
    },
    "dependencies": {
        "all-the-package-names": "^2.0.2275",
        "axios": "^1.13.2",
        "express": "^5.1.0",
        "uuid": "^9.0.1",
        "winston": "^3.18.3",
        "yargs": "^18.0.0"
    },
    "devDependencies": {
        "@types/express": "^5.0.5",
        "@types/jest": "^30.0.0",
        "@types/node": "^20.10.0",
        "@types/uuid": "^9.0.7",
        "@types/yargs": "^17.0.35",
        "@typescript-eslint/eslint-plugin": "^8.48.0",
        "@typescript-eslint/parser": "^8.48.0",
        "eslint": "^8.54.0",
        "jest": "^30.2.0",
        "nodemon": "^3.1.11",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.4.5",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.9.3"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "xregistry",
        "npm",
        "package-registry",
        "typescript"
    ],
    "author": "Your Name",
    "license": "MIT"
}
